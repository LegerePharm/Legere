<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="report_legere_invoice_record" model="ir.actions.report">
            <field name="name">Invoice</field>
            <field name="model">legere.invoice</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">legere_legacy.report_legere_invoice</field>
            <field name="report_file">legere_legacy.report_legere_invoice</field>
            <field name="print_report_name">'Invoice_%s' % (object.InvoiceNumber or '')</field>
            <field name="binding_model_id" ref="model_legere_invoice"/>
            <field name="binding_type">report</field>
        </record>

        <record id="report_legere_order_record" model="ir.actions.report">
            <field name="name">Order</field>
            <field name="model">legere.order</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">legere_legacy.report_legere_order</field>
            <field name="report_file">legere_legacy.report_legere_order</field>
            <field name="print_report_name">'Order_%s' % (object.OrderNumber or '')</field>
            <field name="binding_model_id" ref="model_legere_order"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>